{
  "private": true,
  "scripts": {
    "build": "vue-tsc && nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "format": "prettier --write ./presentation ./logics ./resources ./core ./app",
    "lint": "vue-tsc && eslint ./core ./logics ./presentation ./resources --ext .vue,.js,.ts",
    "lint-fix": "vue-tsc && eslint ./core ./logics ./presentation ./resources --fix .vue,.js,.ts",
    "prepare": "husky install",
    "add-lint-staged": "husky add .husky/pre-commit 'npx lint-staged'"
  },
  "devDependencies": {
    "@nuxtjs/eslint-module": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vue/eslint-config-typescript": "^11.0.2",
    "add": "^2.0.6",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "nuxt": "3.0.0-rc.13",
    "prettier": "2.7.1",
    "sass": "^1.56.1",
    "typescript": "^4.8.4",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^1.0.9"
  },
  "dependencies": {
    "vui18n": "^1.1.25"
  }
}
